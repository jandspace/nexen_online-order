@import url('https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800&subset=cyrillic,cyrillic-ext,greek,greek-ext,latin-ext,vietnamese');


html, body {
	font-family: 'Open Sans',sans-serif;

	div,
	span,
	p,
	li {
		font-size: 12px;
		@mixin md-min {
			font-size: 13px;
		}
		@mixin lg-min {
			font-size: 13px;
		}
	}
}